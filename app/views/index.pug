extends layout

block content
  h1= title
  p Welcome to #{title}
  div.row
    each val, index in data
      div.col-lg-3
        div.border.p-3.mb-3.text-center
          div.mb-2
            img.img-fluid(src=`/images/cat/${val.image}`)
          h4.h6
            a(href=`/category/${val.url}`)= val.title
